{
  "compilerOptions": {
    "target": "es2022",
    "outDir": "dist",
    "module": "esnext",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "sourceMap": true,
    "rootDir": "src"
  },
  "exclude": [
    "./tsup.config.ts",
  ]
}
